/* modal */
body.modal-opened {
  overflow: hidden !important;
}
.modal-wrap {
  z-index: 300;
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  align-self: safe center;
  align-items: center;
  width: 100%;
  height: 100%;
  overflow-y: auto;
  background-color: rgba(0, 0, 0, 0);
  visibility: hidden;
  transition: all 0.3s;
}
.modal-wrap.opened {
  visibility: visible;
  background-color: rgba(0, 0, 0, 0.5);
}
.modal-box {
  /* max-width: 80%; */
  margin: auto;
  opacity: 0;
  transform: scale(0.9);
  transition: opacity 0.3s, transform 0.3s;
}
.modal-wrap.opened .modal-box {
  opacity: 1;
  transform: scale(1);
}
.modal-pop {
  position: relative;
  box-sizing: border-box;
  min-width: 400px;
  margin: 40px;
  padding: 32px;
  background-color: white;
  border-radius: 4px;
}
.pop-head {
  display: flex;
  margin: -32px -32px 32px;
  padding: 24px 32px;
  color: #2d2e42;
  border-bottom: 1px solid #eee;
}
.pop-head > h2 {
  flex: 1;
  margin: 0;
  font-size: 24px;
  line-height: 1.1667;
}
.pop-head > .close {
  width: 32px;
  height: 32px;
  margin-left: 32px;
  text-indent: -9999px;
  background: white url(http://testh.kiha21.or.kr/assets/images/ic_32px.png) no-repeat;
  background-size: auto 32px;
}
.pop-foot {
  margin-top: 32px;
  padding-top: 24px;
  border-top: 1px solid #eee;
  justify-content: flex-end;
}
.pop-foot > .button-group {
  margin-top: 0;
}

/* sitemap (modal) */
.modal-box.sitemap-box {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  transform: scale(1);
}
.sitemap {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 80px 0 0 420px;
  border-radius: 0;
  transition: padding 0.3s;
}
.sitemap > aside {
  position: fixed;
  top: 0;
  left: 0;
  box-sizing: border-box;
  width: 420px;
  height: 100%;
  min-height: 640px;
  padding: 168px 40px 0;
  background: #3b3a9f url(http://testh.kiha21.or.kr/assets/images/logo_sitemap.png) no-repeat 40px 80px;
  background-size: auto 40px;
  transition: left 0.3s;
}
.sitemap > aside > p {
  font-family: "GmarketSansBold";
  font-size: 48px;
  line-height: 52px;
  font-weight: 700;
  color: #eee;
}
.sitemap > aside > ul {
  position: absolute;
  bottom: 64px;
  left: 40px;
}
.sitemap > aside > ul > li > a {
  display: block;
  padding: 8px 0;
  font-size: 18px;
  font-weight: 400;
  color: rgba(238, 238, 238, 0.6);
}
.sitemap > aside > ul > li > a:hover {
  color: #eee;
}
.sitemap > .head {
  position: fixed;
  left: 420px;
  top: 0;
  width: calc(100% - 420px);
  height: 80px;
  background-color: white;
  transition: left 0.3s, width 0.3s;
}
.sitemap > .head > .close {
  position: absolute;
  right: 8px;
  top: 8px;
  box-sizing: border-box;
  width: 64px;
  height: 64px;
  text-indent: -9999px;
  background: white url(http://testh.kiha21.or.kr/assets/images/ic_32px.png) no-repeat;
  background-size: auto 32px;
  border: 16px solid white;
}
.sitemap > .body {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  overflow: auto;
  padding-left: 80px;
  transition: padding-left 0.3s;
  scrollbar-color: transparent #ccc #aaa;
  scrollbar-width: thin;
}
.sitemap > .body > h2 {
  margin: 0;
  font-size: 40px;
  line-height: 1;
}
.sitemap > .body > dl {
  display: flex;
  flex-wrap: wrap;
  width: 1126px;
  padding-right: 40px;
}
.sitemap > .body > dl > dt {
  box-sizing: border-box;
  width: 200px;
  padding: 40px 0;
  font-size: 32px;
  line-height: 36px;
  font-weight: 700;
  color: #3988ff;
}
.sitemap > .body > dl > dd {
  box-sizing: border-box;
  width: calc(100% - 200px);
  padding: 40px 0;
  border-bottom: 1px solid #eee;
}
.sitemap > .body > dl > dd:last-child {
  padding-bottom: 80px;
  border-bottom: none;
}
.sitemap > .body > dl > dd > ul {
  display: flex;
  /* flex-wrap: wrap; */
}
.sitemap > .body > dl > dd > ul > li {
  min-width: 166px;
  margin-right: 24px;
}
.sitemap > .body > dl > dd > ul > li:last-child {
  margin-right: 0;
}
.sitemap > .body > dl > dd > ul > li > a,
.sitemap > .body > dl > dd > ul > li > span {
  display: block;
  font-size: 16px;
  line-height: 40px;
  font-weight: 500;
}
.sitemap > .body > dl > dd > ul > li > ul > li > a {
  display: block;
  font-size: 14px;
  line-height: 32px;
  color: #444;
  white-space: nowrap;
}
.sitemap > .body a:hover {
  color: #3b4396;
}
.sitemap > .body a.outlink:after {
  content: "";
  display: inline-block;
  width: 16px;
  height: 16px;
  margin: -2px 0 0 2px;
  vertical-align: middle;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_outlink.png) no-repeat -32px 0;
  background-size: auto 16px;
}
.sitemap > .body a.outlink:hover:after {
  background-position: -48px 0;
}

.sitemap > .body::-webkit-scrollbar {
  width: 5px;
  height: 5px;
}
.sitemap > .body::-webkit-scrollbar-track {
  background: transparent;
}
.sitemap > .body::-webkit-scrollbar-thumb {
  background: #ccc;
  border-radius: 3px;
}
.sitemap > .body::-webkit-scrollbar-thumb:hover {
  background: #aaa;
}
@media (max-width: 1631px) {
  .sitemap {
    padding-left: 0;
  }
  .sitemap > aside {
    left: -420px;
  }
  .sitemap > .head {
    left: 0;
    width: 100%;
  }
  .sitemap > .body {
    padding-left: 40px;
  }
  .sitemap > .body > dl {
    width: 1106px;
  }
  .sitemap > .body > dl > dt {
    width: 180px;
  }
  .sitemap > .body > dl > dd {
    width: calc(100% - 180px);
  }
}

