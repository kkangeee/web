#m_nav {
    display: none;
    position: absolute;
    top: 0;
    right: 0;
    width: 10%;
    height: 500px;
    background-color: #fff;
    z-index: 1002;
    box-sizing: border-box;
}

#m_nav .deps1 {
    position: absolute;
    right: 0;
    width: 100%;
}

#m_nav .deps1 > li {
    padding: 0 30px;
}

#m_nav .deps1 > li > a {
    display: block;
    width: 100%;
    font-size: 17px;
    font-weight: 500;
    color: #888;
    padding: 12px 20px;
    box-sizing: border-box;
    border-bottom: 1px solid #ddd;
}

#m_nav .deps1 > li > a:hover {
    color: #0080ff;
}

#m_nav .title {
    color: #fff;
    background: #2aaeeb;
    font-size: 34px;
    padding: 26px 33px !important;
}

#m_nav .close_icon {
    overflow: hidden;
    text-indent: -999px;
    width: 40px;
    height: 40px;
    position: absolute;
    top: 20px;
    left: -80px;
    background: url('http://cfile25.uf.tistory.com/image/990FAC33599FC712190B56') no-repeat 0 0;
    cursor: pointer;
}

