  line-height: 48px;
  color: #444;
}
/*pop창 관련*/
#pop_dataSection > tr:hover{
	color: blue;
}
/* gnb */
#gnb {
  pointer-events: none;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}
#gnb > nav {
  box-sizing: border-box;
  max-width: 1440px;
  overflow: hidden;
  padding: 0 36px 0 248px;
  margin: 0 auto;
}
#gnb > nav > ul:first-child {
  pointer-events: all;
  display: flex;
  float: left;
}
#gnb > nav > ul:first-child:after {
  content: "";
  display: block;
  clear: both;
}
#gnb > nav > ul:first-child > li {
}
#gnb > nav > ul:first-child > li > a {
  position: relative;
  display: block;
  padding: 0 20px;
  font-size: 18px;
  line-height: 96px;
  font-weight: 500;
  color: #222;
  white-space: nowrap;
}
#gnb > nav > ul:first-child > li > a:before {
  content: "";
  position: absolute;
  left: 20px;
  bottom: 0;
  width: 0;
  height: 3px;
  background: linear-gradient(to left, transparent 40px, #3b4396 40px); /* for IE11 (calc() transition 미지원) */
  transition: width 0.3s;
}
#gnb > nav > ul:first-child:not(:hover) > li.active > a,
#gnb.disabled > nav > ul:hover > li.active > a,
#gnb:not(.disabled) > nav > ul:first-child > li:hover > a {
  color: #3b4396;
}
#gnb > nav > ul:first-child:not(:hover) > li.active > a:before,
#gnb.disabled > nav > ul:hover > li.active > a:before,
#gnb:not(.disabled) > nav > ul:first-child > li:hover > a:before {
  width: 100%;
}
#gnb a.outlink:after {
  content: "";
  display: inline-block;
  width: 16px;
  height: 16px;
  margin: -3px 0 0 8px;
  vertical-align: middle;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_outlink.png) no-repeat;
  background-size: auto 16px;
}
#gnb > nav > ul:first-child > li.active > a.outlink:after,
#gnb:not(.disabled) > nav > ul:first-child > li:hover > a.outlink:after {
  background-position: -16px 0;
}

#gnb .gnb-spread {
  pointer-events: none;
  z-index: 1;
  position: absolute;
  left: 0;
  top: 100%;
  box-sizing: border-box;
  width: 100%;
  min-width: 1024px;
  padding: 0 36px;
  background-color: white;
  border: 1px solid #eee;
  border-width: 1px 0;
  box-shadow: 0 12px 12px 0 rgba(0, 0, 0, 0.04);
  opacity: 0;
  transition: opacity 0.1s;
  transition-delay: 0.1s;
}
#gnb:not(.disabled) li:hover > .gnb-spread {
  transition-delay: 0s;
  pointer-events: all;
  z-index: 30;
  opacity: 1;
}
@media (hover: none) {
  /* 터치디바이스에서 hover 시 노출 없음 */
  #gnb .gnb-spread {
    display: none;
  }
}
#gnb .gnb-spread a,
#gnb .gnb-spread li > span {
  opacity: 0;
  transform: translateX(-8px);
  transition: opacity 0.2s, transform 0.2s;
  transition-delay: 0.2s;
}
#gnb:not(.disabled) li:hover > .gnb-spread a,
#gnb:not(.disabled) li:hover > .gnb-spread li > span {
  transition-delay: 0s;
  opacity: 1;
  transform: translateX(0);
}
#gnb .gnb-spread > ul {
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  max-width: 1368px;
  margin: -8px auto 40px;
  padding-left: 232px;
}
#gnb .gnb-spread > ul > li {
  width: 208px;
  margin: 32px 24px 0 0;
}
#gnb .gnb-spread > ul > li > a,
#gnb .gnb-spread > ul > li > span {
  display: block;
  font-size: 16px;
  line-height: 40px;
  font-weight: 500;
}
#gnb .gnb-spread > ul > li > ul {
  display: flex;
  flex-wrap: wrap;
}
#gnb .gnb-spread > ul > li > ul > li {
  width: 208px;
}
#gnb .gnb-spread > ul > li > ul > li > a {
  display: block;
  font-size: 14px;
  line-height: 32px;
  color: #444;
}
#gnb .gnb-spread li.active > a,
#gnb .gnb-spread a:hover {
  color: #3b4396;
}
#gnb .gnb-spread a.outlink:after {
  margin-left: 2px;
  background-position: -32px 0;
}
#gnb .gnb-spread li.active > a.outlink:after,
#gnb .gnb-spread a.outlink:hover:after {
  background-position: -48px 0;
}
#gnb .gnb-spread > ul > li.full {
  width: 100%;
}
#gnb .gnb-spread > ul > li.full > ul > li {
  margin-right: 24px;
}

#gnb .util {
  pointer-events: all;
  display: flex;
  float: right;
  margin: 28px 0 0 -100%;
}
#gnb .util > li {
  margin-left: 16px;
}
#gnb .util > li > button {
  width: 40px;
  height: 40px;
  text-indent: -9999px;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_nav.png) no-repeat;
  background-size: auto 40px;
}
#gnb .util > li.search > button {
}
#gnb .util > li.map > button {
  background-position: -40px 0;
}
/* location */
#location {
  z-index: 50;
  position: sticky;
  top: 0;
  background-color: white;
  border-bottom: 1px solid transparent;
}
#location.fixed {
  border-color: #eee;
  box-shadow: 0 12px 12px 0 rgba(0, 0, 0, 0.04);
}
#location > ul {
  display: flex;
  box-sizing: border-box;
  max-width: 1440px;
  margin: auto;
  padding: 0 36px;
}
#location > ul > li {
  position: relative;
}
#location > ul > li:not(:first-child):before {
  content: "";
  position: absolute;
  left: 0;
  top: 50%;
  width: 6px;
  height: 14px;
  margin-top: -7px;
  background: url(/images/ic_16px.png) no-repeat -48px 0;
  background-size: auto 16px;
}
#location > ul > li > a,
#location > ul > li > button,
#location > ul > li > span {
  position: relative;
  display: inline-block;
  padding: 0 51px 0 32px;
  line-height: 43px;
  background-color: transparent;
}
#location > ul > li > a {
  min-width: 24px;
  padding: 0 25px;
  text-indent: -9999px;
}
#location > ul > li > a:before {
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  width: 24px;
  height: 24px;
  margin: -12px 0 0 -12px;
  background: url(/images/images/ic_24px.png) no-repeat -120px 0;
  background-size: auto 24px;
}
#location > ul > li > span {
  padding-right: 32px;
}
#location > ul > li > button:after {
  position: absolute;
  top: 50%;
  width: 16px;
  height: 16px;
  margin: -8px 0 0 10px;
  background: url(/images/select.png) no-repeat;
  background-size: 16px;
  transition: transform 0.3s;
}

#location > ul > li > .locationbtn_h_no {
  pointer-events:none;
}

#location > ul > li > button.active:after {
  transform: rotate(-180deg);
}
#location > ul > li > ul {
  position: absolute;
  display: none;
  left: 32px;
  top: calc(100% + 1px);
  box-sizing: border-box;
  min-width: 200px;
  padding: 12px 0;
  background: white;
  border: 1px solid #888;
  /* box-shadow: 0 16px 24px 0 rgba(0, 0, 0, .08); */
}
#location > ul > li > ul > li > a {
  display: block;
  padding: 0 16px;
  font-size: 14px;
  line-height: 32px;
  white-space: nowrap;
}
#location > ul > li > ul > li > a:hover {
  background-color: #f8f8f8;
}

/* search */
.search-wrap {
  pointer-events: none;
  z-index: 300;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow-y: auto;
  background-color: rgba(0, 0, 0, 0);
  visibility: hidden;
  transition: all 0.3s;
}
.search-wrap.opened {
  pointer-events: all;
  visibility: visible;
  background-color: rgba(0, 0, 0, 0.5);
}
.search-box {
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  min-width: 1190px;
  height: 160px;
  background-color: white;
  transform: translateY(-100%);
  transition: transform 0.3s;
}
.search-wrap.opened .search-box,
#content .search-box {
  transform: translateY(0);
}
#content .search-box {
  position: static;
  height: auto;
  margin: 40px 0;
}
.search-box .close {
  position: absolute;
  right: 48px;
  top: 50%;
  box-sizing: border-box;
  width: 64px;
  height: 64px;
  margin-top: -32px;
  text-indent: -9999px;
  background: white url(http://testh.kiha21.or.kr/assets/images/ic_32px.png) no-repeat;
  background-size: auto 32px;
  border: 16px solid white;
}
.search-bar {
  display: flex;
  align-items: center;
  border-bottom: 2px solid #3b3a9f;
}
.search-bar > form {
  display: flex;
  align-items: center;
}
.search-bar input[type="text"] {
  width: 500px;
  height: 64px;
  padding: 0;
  font-size: 24px;
  font-weight: 500;
  border: none;
  outline: none;
}
.search-bar input::-webkit-input-placeholder {
  font-weight: 500;
  color: #ccc !important;
}
.search-bar input::-moz-placeholder {
  font-weight: 500;
  color: #ccc !important;
}
.search-bar input:-ms-input-placeholder {
  font-weight: 500;
  color: #ccc !important;
}
.search-bar button[type="submit"] {
  height: 48px;
  width: 48px;
  margin: 0 8px;
  text-indent: -9999px;
  background: white url(http://testh.kiha21.or.kr/assets/images/ic_48px.png) no-repeat;
  background-size: auto 48px;
}

.search-counter {
