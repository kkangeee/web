@charset "utf-8";
@font-face {
  font-family: "GmarketSansBold";
  src: url("https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2001@1.1/GmarketSansBold.woff") format("woff");
  font-weight: normal;
  font-style: normal;
}

@media only print {
 #printBtn, #paginationBox, #location, #totalCnt, #top_main, #gnb, footer, #footer2, header, 
 #searchbox, #ninfo, .select_bd_tit, input[type="checkbox"], .tab_wrap, #insertBtn, #deleteBtn, 
 #updateBtn, #saveBtn, #printBtn, #cancelBtn #top_bar, #content > .button-group {display:none;}  
 #tablePrint, #tBodyPrint > td { font-family: "Noto Sans KR", sans-serif; font-size: 9px;}
 page-break-before : [auto, inherit, always, avoid, left, right]
 page-break-after : [auto, inherit, avoid]
};

@page {
    size: auto;
    size: portrait;
    size: landscape;

    size: 6in; 
    size: 4in 6in;

    size: A4;
    size: B5;
    size: letter;

    size: A4 portrait;
   
}
@page {margin:0}
html,body {  margin: 0;  padding: 0;}
html {
  font-family: "Noto Sans KR", sans-serif;
  font-size: 16px;
  line-height: 1.5;
  color: #222;
  word-break: keep-all;
}

table {
  table-layout: fixed;
  width: 100%;
  font-size: 1em;
  line-height: 1.4615;
  border-collapse: collapse;
  border-spacing: 0;
}
th,td {  word-wrap: break-word;}
a {
  color: transparent; /* for IE11 (polyfill 적용 전 푸른색 방지) */
  color: #222;
  text-decoration: none;
  /* outline: none; */
}
img {
  margin: 0;
  vertical-align: top;
  font-size: 0;
  line-height: 0;
  border: none;
}
ul,ol,li,dl,dt,dd,form,fieldset,legend,pre {  margin: 0;  padding: 0;}
ul,ol {  list-style: none;}
fieldset,legend {  border: none;}
input,select,textarea,button {
  box-sizing: border-box;
  min-height: 16px;
  margin: 0;
  padding: 0;
  /* outline: none; */
  vertical-align: baseline;
  font-family: "Noto Sans KR", sans-serif;
  font-size: 1em;
  color: #222;
}
button {  cursor: pointer;  border: none;  border-radius: 0;}
p {  margin: 0;}
em,i,address {  font-style: normal;}
strong {  font-weight: 500;}
input::-webkit-input-placeholder {font-weight: 300; color: #888 !important;}
input::-moz-placeholder {  font-weight: 300;  color: #888 !important;}
input:-ms-input-placeholder {  font-weight: 300;  color: #888 !important;}
textarea::-webkit-input-placeholder {  font-weight: 300;  color: #888 !important;}
textarea::-moz-placeholder {  font-weight: 300;  color: #888 !important;}
textarea:-ms-input-placeholder {  font-weight: 300;  color: #888 !important;}
input::-ms-clear {  display: none;}

textarea {
  width: 100%;
  resize: none;
  overflow: auto;
  padding: 14px 16px;
  vertical-align: top;
  font-size: 1.125rem;
  color: #222;
  background-color: white;
  border: 1px solid #ccc;
}
input[type="text"],
input[type="email"],
input[type="date"],
input[type="password"] {
  box-sizing: border-box;
  height: 40px;
  padding: 0 16px;
  color: #5c5b5b;
  background-color: white;
  border: 1px solid #ccc;
}
input[type="text"].input_bd_tit,
input[type="email"].input_bd_tit,
input[type="date"].input_bd_tit,
input[type="password"].input_bd_tit {
  height: 30px;
  width: 90px;
  font-size: 15px;
  margin-left: 5px;
}
input[type="text"]:disabled,
textarea:disabled,
select:disabled {background: #f8f8f8}

select {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  height: 56px;
  padding: 0px 15px 0px 25px;
  color: #222;
  background: url(/images/ic_select.png) no-repeat right 16px top 50%;
  background-size: 16px;
  border: 1px solid #ccc;
}
select.select_bd_tit {
    height: 30px;
    padding: 0 25px 0 9px;
    font-size: 15px;
    background: #fff url(/images/ic_select.png) no-repeat right 6px top 50%;
    background-size: 16px;
}

select::-ms-expand {  display: none;}
select:required:invalid {  color: #888 !important;}
option[value=""][disabled] {  display: none;}
option {  color: #222;}

label {  display: inline-block;  margin: 12px 0;  white-space: nowrap;}
label > em {  color: #2064f9;}
label > i {  color: #888;}
input[type="checkbox"],input[type="radio"] {  width: 20px;  height: 20px;}
.input-checkbox {
  position: relative;
  display: inline-block;
  width: 20px;
  height: 20px;
}
label > .input-checkbox {  margin: 0 2px -4px 0;}
.input-checkbox > input {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.input-checkbox > input + i {
  pointer-events: none;
  position: absolute;
  left: 0;
  top: 0;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: white;
  border: 1px solid #ccc;
  border-radius: 2px;
}
.input-checkbox > input[type="radio"] + i {  border-radius: 50%;}
.input-checkbox > input:focus + i {  border-color: #3988ff;}
.input-checkbox > input[type="checkbox"]:checked + i {
  background: #3988ff url(/images/ic_16px.png) no-repeat -192px 0;
  background-size: auto 16px;
  border: 2px solid #3988ff;
}
.input-checkbox > input[type="radio"]:checked + i:before {
  content: "";
  display: block;
  box-sizing: border-box;
  width: calc(100% + 2px);
  height: calc(100% + 2px);
  margin: -1px; /* for IE11 (background-color 삐져나옴 가리기, background-clip은 원이 이쁘지 않음) */
  background-color: #3988ff;
  border: 5px solid white;
  border-radius: 50%;
}

.checkbox-group {  display: flex;  margin: 0px;}
.checkbox-group > label {  margin: 4px 10px 4px 0 !important;}

#content input:not(:last-child),
#content select:not(:last-child),
#content button:not(:last-child),
#content label:not(:last-child),
#content .input-button:not(:last-child),
.input-sep {  margin-right: 5px;}
.input-sep {  font-weight: 400;}

/* input-button */
.input-button {
  display: inline-flex;
  align-items: center;
  box-sizing: border-box;
  padding-right: 8px;
  border: 1px solid #ccc;
}
.input-button > input {
  flex: 1;
  min-width: 0;
  height: 54px;
  border: none;
}
.input-button > button {
  height: 40px;
  padding: 0 16px;
  font-size: 1rem;
  font-weight: 400;
  background-color: #eee;
}

/* input-file */
.input-file {
  position: relative;
  display: inline-block;
  margin: 8px 20px 8px 0;
}
.input-file > input[type="file"] {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding-left: 100%;
  opacity: 0;
  cursor: pointer;
}
.input-file > button {
  height: 40px;
  margin: 0;
  padding: 0 16px;
  font-size: 1rem;
  font-weight: 400;
  background-color: #eee;
}
.attached-files {
  display: inline-block;
  margin-bottom: 6px;
}
.attached-files:only-child {
  margin: 14px 0;
}
.attached-files > i {
  display: inline-block;
  font-size: 1rem;
}
.attached-files > i:not(:last-child) {
  margin-right: 20px;
}
.attached-files > i > a {
  color: #1a75ff;
}
.attached-files > i > a:hover {
  text-decoration: underline;
}
.attached-files > i > .delete {
  width: 16px;
  height: 100%;
  margin-left: 2px;
  text-indent: -9999px;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_16px.png) no-repeat -64px 50%;
  background-size: auto 16px;
}

/* button-group */
.board-write button.primary,
.board-write button.secondary {
  box-sizing: border-box;
  min-width: 120px;
  height: 56px;
  margin: 0;
  padding: 0 32px;
  vertical-align: top;
  text-align: center;
  font-size: 1.125rem;
  line-height: 54px;
  border: 1px solid transparent;
}

.button-group {  display: flex;}
.button-group.top {
  /*justify-content: flex-end;*/
  margin-bottom: 16px;
}
.button-group.bottom {
  justify-content: center;
  margin-top: 32px;
}
.button-group > button,
.button-group > a {
  box-sizing: border-box;
  min-width: 110px;
  height: 56px;
  margin: 0;
  padding: 0 32px;
  text-align: center;
  font-size: 1rem;
  line-height: 54px;
  border: 1px solid transparent;
}
.button-group.bottom > button,
.button-group.bottom > a {
  font-size: 1.125rem;
  min-width: 120px;
}
.button-group.top > button:not(:first-child),
.button-group.top > a:not(:first-child) {
  margin-left: 8px;
  margin-right: 0 !important;
}
.button-group.bottom > button:not(:last-child),
.button-group.bottom > a:not(:last-child) {
  margin-right: 12px !important;
}
.button-group p.allNB {
  line-height: 56px;
  width: 100%;
}
button.primary,
a.primary {
  color: white;
  background-color: #535cbb;
}
button.primary:not(:disabled):hover,
a.primary:not(:disabled):hover {
  background-color: #3b4396;
}
button.secondary,
a.secondary {
  color: #222;
  background-color: white;
  border-color: #ccc !important;
}
button.secondary:not(:disabled):hover,
a.secondary:not(:disabled):hover {
  background-color: #fbfbfb;
}

/* individual */
.ta-left {
  text-align: left !important;
}
.ta-center {
  text-align: center !important;
}
.ta-right {
  text-align: right !important;
}
.fw-normal {
  font-weight: 400 !important;
}
.fw-bold {
  font-weight: 500 !important;
}

#wrap {  box-sizing: border-box;  min-width: 1190px;}

header {
  z-index: 101;
  position: relative;
  background-color: white;
  border-width: 42px 0 1px;
  border-style: solid;
  border-color: transparent;
  /*border-top-color: #fbfbfb;*/
