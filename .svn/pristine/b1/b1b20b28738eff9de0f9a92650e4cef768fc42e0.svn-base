/* main popup */
.popup-wrap {
  z-index: 300;
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  align-self: safe center;
  align-items: center;
  width: 100%;
  height: 100%;
  overflow-y: auto;
  background-color: rgba(0, 0, 0, 0.5);
}
.popup-wrap .popups {
  width: 100%;
  max-width: 1452px;
  margin: auto;
  padding: 40px;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.popup-wrap .popups:after {
  /* for IE11 */
  content: "";
  width: 100%;
  height: 96px;
}
.popup-wrap .popups > i {
  width: 480px;
  margin: 0 2px;
  display: none;
}
.popup-wrap .popup + .popup + .popup + i,
.popup-wrap .popup + .popup + .popup + i + i {
  display: block;
}
.popup-wrap .popup {
  margin: 4px 2px;
}
.popup-wrap .popup > img,
.popup-wrap .popup > a > img {
  width: 480px;
  height: 640px;
}
.popup-wrap .footer {
  display: flex;
  justify-content: space-between;
  background-color: #222;
}
.popup-wrap .footer > button {
  padding: 0 8px;
  height: 32px;
  font-size: 12px;
  line-height: 32px;
  color: #fff;
  background: none;
}
.popup-wrap .footer .stop:before {
  content: "";
  display: inline-block;
  width: 16px;
  height: 16px;
  margin: 8px 4px 0 0;
  vertical-align: top;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_16px.png) no-repeat -32px 0;
  background-size: auto 16px;
}
.popup-wrap .footer .close {
  width: 32px;
  text-indent: -9999px;
  background: url(http://testh.kiha21.or.kr/assets/images/ic_32px.png) no-repeat -96px 0;
  background-size: auto 32px;
}
.popup-wrap .bottom {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.8);
}
.popup-wrap .bottom > button {
  height: 56px;
  padding: 0 24px;
  font-size: 16px;
  line-height: 56px;
  color: white;
  background: none;
}
.popup-wrap .bottom .sep {
  width: 1px;
  height: 20px;
  text-indent: -9999px;
  background-color: rgba(255, 255, 255, 0.3);
}

