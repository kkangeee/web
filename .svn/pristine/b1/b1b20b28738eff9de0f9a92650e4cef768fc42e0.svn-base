#fade {
    display: none;
    position: fixed;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
    background-color: #ffffff;
    z-index: 1001;
    /* -moz-opacity: 0.8; */
    opacity: 0;
    filter: alpha(opacity=80);
}

#fade .close {
    display: block;
    position: absolute;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
}
